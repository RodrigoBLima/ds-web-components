{
  "name": "@rodrigoblima/ds-web-components",
  "version": "1.0.15",
  "keywords": [
    "ui",
    "ui-kit"
  ],
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/ds-web-components/ds-web-components.esm.js",
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "storybook": "stencil build && storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build",
    "storybook:deploy": "gh-pages -d storybook-static",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "lint": "eslint ./src --ext .ts,.tsx",
    "lint:fix": "prettier --write \"src/**/*.{ts,tsx,js,css,json}\"",
    "pre-commit": "npm --no-git-tag-version version patch && git add .",
    "prepare": "husky",
    "release": "semantic-release"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RodrigoBLima/ds-web-components.git"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/ds-web-components/ds-web-components.esm.js",
      "require": "./dist/cjs/ds-web-components.cjs.js",
      "default": "./dist/ds-web-components/ds-web-components.esm.js"
    },
    "./components/*": {
      "import": "./dist/components/*.js",
      "types": "./dist/components/*.d.ts"
    },
    "./loader": {
      "import": "./dist/loader/index.js",
      "require": "./dist/loader/index.cjs.js"
    },
    "./vue": {
      "import": "./dist/vue/components.ts"
    },
    "./hydrate": {
      "types": "./dist/hydrate/index.d.ts",
      "import": "./dist/hydrate/index.mjs",
      "require": "./dist/hydrate/index.js",
      "default": "./dist/hydrate/index.js"
    },
    "./react": {
      "import": "./dist/react/components.ts"
    }
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@stencil/core": "4.36.2"
  },
  "peerDependencies": {
    "@stencil/react-output-target": "^1.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.32.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@stencil/react-output-target": "^1.2.0",
    "@stencil/sass": "^3.2.1",
    "@stencil/vue-output-target": "^0.11.8",
    "@storybook/addon-docs": "^8.5.3",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-links": "^8.5.3",
    "@storybook/blocks": "^8.5.3",
    "@storybook/manager-api": "^8.5.3",
    "@storybook/test": "^8.5.3",
    "@storybook/theming": "^8.5.3",
    "@storybook/web-components": "^8.6.14",
    "@storybook/web-components-vite": "^8.6.14",
    "@tailwindcss/cli": "^4.1.3",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "lit-html": "^3.3.1",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "puppeteer": "^24.3.0",
    "semantic-release": "^24.2.7",
    "stencil-tailwind-plugin": "^2.0.3",
    "storybook": "^8.6.14",
    "tailwindcss": "^4.1.3",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vite": "^4.5.14"
  }
}
